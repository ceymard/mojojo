{
  "type": "dark",
  "colors": {
    "foreground": "#EEEEEE",
    "widget.shadow": "#1F1C21",
    "activityBar.background": "#131115",
    "activityBar.foreground": "#777777",
    "activityBar.border": "#1D1A1F",
    "activityBarBadge.background": "#B438F7",
    "activityBarBadge.foreground": "#600692",
    "sideBar.background": "#131115",
    "sideBar.foreground": "#EEEEEE",
    "sideBar.border": "#1D1A1F",
    "sideBarSectionHeader.background": "#1D1A1F",
    "sideBarSectionHeader.foreground": "#EEEEEE",
    "editorGroupHeader.tabsBackground": "#131115",
    "tab.inactiveBackground": "#131115",
    "tab.inactiveForeground": "#777777",
    "tab.activeBackground": "#272229",
    "tab.activeForeground": "#EEEEEE",
    "tab.border": "#1D1A1F",
    "editor.foreground": "#EEEEEE",
    "editor.background": "#131115",
    "editor.lineHighlightBackground": "#240237",
    "editor.selection": "#600692",
    "editor.selectionBorder": "#300349",
    "editorBracketMatch.background": "#7907B6",
    "editorBracketMatch.border": "#6C07A4",
    "statusBar.background": "#600692",
    "editorIndentGuide.background": "#1B181D"
  },
  "tokenColors": [
    {
      "scope": "comment, punctuation.definition.comment, string.quoted.docstring, string.quoted.docstring punctuation.definition.string",
      "settings": {
        "foreground": "#664479",
        "background": "#33223D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.control, keyword.operator.expression, keyword.operator.new, keyword.other, storage.type, keyword.operator.logical.python, storage.modifier",
      "settings": {
        "foreground": "#CF81FA",
        "background": "#7907B6"
      }
    },
    {
      "scope": "string variable, string punctuation.accessor, string keyword",
      "settings": {
        "foreground": "#EEEEEE",
        "background": "#777777"
      }
    },
    {
      "scope": "meta.objectliteral punctuation.definition, meta.objectliteral meta.object-literal.key, meta.class variable.object.property",
      "settings": {
        "foreground": "#E4B8FC",
        "background": "#8B08D2"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#E4B8FC",
        "background": "#8B08D2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#E4B8FC",
        "background": "#8B08D2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.type.annotation, entity.name.type, meta.return.type, meta.type.parameters, entity.other.inherited-class, support.type, markup.list beginning.punctuation",
      "settings": {
        "foreground": "#81D4FA",
        "background": "#077FB6"
      }
    },
    {
      "scope": "meta.indexer.declaration variable.parameter, meta.object.type punctuation.definition, meta.type.annotation variable.object.property, punctuation.definition.typeparameters, meta.type.annotation keyword, meta.type.annotation meta.brace, meta.type.annotation punctuation, meta.type.parameters punctuation, meta.type.parameters meta.brace, markup.quote",
      "settings": {
        "foreground": "#CAEDFD",
        "background": "#0999DB"
      }
    },
    {
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#38BBF7",
        "background": "#066692"
      }
    },
    {
      "scope": "meta.definition entity.name.function, entity.name.function, support.function, markup.heading",
      "settings": {
        "foreground": "#CF81FA",
        "background": "#7907B6"
      }
    },
    {
      "scope": "meta.definition.variable, meta.parameters punctuation, meta.parameters punctuation.definition, variable.parameter, markup.heading punctuation",
      "settings": {
        "foreground": "#E4B8FC",
        "background": "#8B08D2"
      }
    },
    {
      "scope": "entity.name.tag, markup.inline punctuation, markup.fenced_code punctuation",
      "settings": {
        "foreground": "#D4FA81",
        "background": "#7FB607"
      }
    },
    {
      "scope": "entity.other.attribute-name, meta.tag punctuation.section, meta.tag punctuation.definition, meta.tag keyword.operator.assignment, markup.inline, markup.fenced_code",
      "settings": {
        "foreground": "#E0FBA5",
        "background": "#8CC808"
      }
    },
    {
      "scope": "meta.embedded string, punctuation.definition.string, string",
      "settings": {
        "foreground": "#99E2BC",
        "background": "#2A935D"
      }
    },
    {
      "scope": "meta.link.inline.markdown, meta.link.inline.markdown punctuation.definition.string, meta.image.inline, meta.image.inline punctuation.definition.string",
      "settings": {
        "foreground": "#D4F3E3",
        "background": "#33B16F"
      }
    },
    {
      "scope": "string punctuation.definition.template-expression",
      "settings": {
        "foreground": "#2DDD81",
        "background": "#137240"
      }
    },
    {
      "scope": "constant, variable.language",
      "settings": {
        "foreground": "#FCB8B9",
        "background": "#D2080C"
      }
    }
  ]
}
